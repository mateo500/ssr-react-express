{
  "env": {
    "browser": true,
    "cypress/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "prettier",
    "plugin:jest/recommended",
    "plugin:cypress/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "import", "immutable", "jest", "cypress"],
  "rules": {
    "import/prefer-default-export": "off",
    "spaced-comment": "off",
    "camelcase": "off",
  }
}
